{
    "name": "wyrihaximus/js-compress",
    "description": "Compress/minify your JavaScript",
    "keywords": [
        "html",
        "compress"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "linkorb/jsmin-php": "1.0.0",
        "matthiasmullie/minify": "^1.3",
        "meenie/javascript-packer": "1.1",
        "tedivm/jshrink": "^1.3",
        "wyrihaximus/compress": "^1.2 || ^2.0",
        "wyrihaximus/compress-contracts": "^1.0",
        "wyrihaximus/constants": "^1.6"
    },
    "require-dev": {
        "wyrihaximus/compress-test-utilities": "^1.0",
        "wyrihaximus/test-utilities": "^3.3.1"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\JsCompress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\JsCompress\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
